{
    "application": {
        "single_instance_guid": ""
    },
    "debugging": {
        "show_console": true,
        "log_level": "DEBUG4",
        "log_file": "debug.log"
    },
    "main_window": {
        "title": "PHP Desktop Chrome",
        "icon": "",
        "default_size": [800, 600],
        "minimum_size": [800, 600],
        "maximum_size": [0, 0],
        "disable_maximize_button": false,
        "center_on_screen": true
    },
    "popup_window": {
        "icon": "",
        "fixed_title": ""
    },
    "web_server": {
        "listen_on": ["127.0.0.1", 54007],
        "www_directory": "../www",
        "index_files": ["index.html", "index.php"],
        "cgi_interpreter": "../php/php-cgi.exe",
        "cgi_extensions": ["php"]
    }
}
